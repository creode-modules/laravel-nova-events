{
    "name": "creode/laravel-nova-events",
    "description": "Add Events wth categories to your Laravel Nova project.",
    "authors": [
        {
            "name": "Creode",
            "email": "contact@creode.co.uk",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/creode-modules/laravel-nova-events",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Creode\\LaravelNovaEvents\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creode\\LaravelNovaEvents\\ToolServiceProvider",
                "Creode\\LaravelNovaEvents\\LaravelNovaEventsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
