{
    "name": "creode/laravel-nova-events",
    "description": "Add Events to your Laravel Nova project.",
    "authors": [
        {
            "name": "Creode",
            "email": "contact@creode.co.uk",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/creode-modules/laravel-nova-events",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "creode/laravel-repository": "^1.0",
        "creode/nova-page-builder": "^1.3",
        "creode/nova-publishable": "^1.0",
        "laravel/nova": "^4.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "orchestra/canvas": "*"
    },
    "autoload": {
        "psr-4": {
            "Creode\\LaravelNovaEvents\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creode\\LaravelNovaEvents\\LaravelNovaEventsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
